{
    "name": "zablockibros/laravel-mappers",
    "description": "Mappers and adapters for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Zablocki",
            "email": "zablockijj@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "~5.7.0",
        "illuminate/database": "~5.7.0",
        "illuminate/support": "~5.7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "ZablockiBros\\Mappers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZablockiBros\\Mappers\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "ZablockiBros\\Mappers\\ComposerScripts::postAutoloadDump"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZablockiBros\\Mappers\\MappersServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
